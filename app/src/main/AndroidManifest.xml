<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khansa.mydictonary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dd"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dd"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.khansa.thedictionary.Word_meaningActivity"
            android:launchMode="singleTask">

            <intent-filter>
                <action android:name="android.intent.action.SEND"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="text/plain"/>
            </intent-filter>
        </activity>
        <activity android:name="com.khansa.thedictionary.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>